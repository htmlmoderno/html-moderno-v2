    "acessibilidade",
    "seo",
    "ux",
    "w3c",
    "wcag",
    "aria"
  ],
  "author": "Alan Ktquez <ktquez@gmail.com> (https://ktquez.dev/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ktquez/html-moderno-v2/issues"
