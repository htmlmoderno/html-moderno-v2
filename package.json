{
  "name": "html-moderno-v2",
  "version": "2.0.0",
  "description": "Blog do projeto HTML Moderno",
  "private": true,
  "scripts": {
    "dev": "node_modules/.bin/vuepress dev src",
    "build": "npm run svgo && npm run buildProject",
    "buildProject": "node_modules/.bin/vuepress build src",
    "svgo": "svgo --disable=convertColors -f ./src/.vuepress/theme/assets/sprite/svg",
    "deploy": "npm run build && firebase deploy",
    "staging": "surge src/.vuepress/dist/ htmlmoderno-2.surge.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ktquez/html-moderno-v2.git"
  },
